{
  "name": "svn-repository-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/svn-repository-provider.mjs",
  "exports": {
    ".": "./src/svn-repository-provider.mjs"
  },
  "description": "provider of subversion repositories",
  "keywords": [
    "repository-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/svn-repository-provider.mjs --section=API && documentation readme ./src/svn-repository-provider.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint src/svn-repository-provider.mjs && documentation lint ./src/svn-repository-provider.mjs"
  },
  "dependencies": {
    "repository-provider": "^23.12.1"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.4",
    "documentation": "^13.0.2",
    "semantic-release": "^17.2.1"
  },
  "engines": {
    "node": ">=12.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svn-repository-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svn-repository-provider/issues"
  },
  "homepage": "https://github.com/arlac77/svn-repository-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
